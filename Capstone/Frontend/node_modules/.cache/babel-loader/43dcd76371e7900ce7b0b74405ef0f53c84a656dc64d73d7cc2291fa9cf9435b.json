card\">\n        <div class=\"brand-section\">\n          <h1 class=\"brand\">Margatheo Store</h1>\n          <p class=\"tagline\">Welcome back! Please login to your account.</p>\n        </div>\n\n        <!-- Main Login Form -->\n        <form @submit.prevent=\"handleLogin\" class=\"auth-form\">\n          <div class=\"form-group\">\n            <label for=\"email\">Email Address</label>\n            <div class=\"input-wrapper\">\n              <span class=\"input-icon\">ðŸ“§</span>\n              <input\n                type=\"email\"\n                id=\"email\"\n                v-model=\"email\"\n                required\n                placeholder=\"Enter your email\"\n                autocomplete=\"email\"\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group\">\n            <label for=\"password\">Password</label>\n            <div class=\"input-wrapper\">\n              <span class=\"input-icon\">ðŸ”’</span>\n              <input\n                type=\"password\"\n                id=\"password\"\n                v-model=\"password\"\n                required\n                placeholder=\"Enter your password\"\n                autocomplete=\"current-password\"\n              />\n            </div>\n          </div>\n\n          <div v-if=\"error\" class=\"error-message\">\n            {{ error }}\n          </div>\n\n          <button type=\"submit\" class=\"auth-button\" :disabled=\"isLoading\">\n            {{ isLoading ? 'Logging in...' : 'Login' }}\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: null,\n      isLoading: false,\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n\n      this.isLoading = true;\n      this.error = null;\n\n      try {\n        console.log('Sending login request...');\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true // Enable cookies for session\n        });\n\n        console.log('Login response:', response);\n\n        if (response.data.needsVerification) {\n          this.error = response.data.message;\n          return;\n        }\n\n        // Store user data in localStorage\n        localStorage.setItem('userEmail', response.data.email);\n        localStorage.setItem('userRole', response.data.role);\n        localStorage.setItem('token', response.data.token); // Store the actual JWT token\n        localStorage.setItem('isLoggedIn', 'true');\n\n        // Redirect based on role\n        const role = response.data.role;\n        const allowedRoles = ['admin', 'Lead architect', 'Secretary', 'Book keeper', 'Liaison', 'Apprentice'];\n        if (allowedRoles.includes(role)) {\n          await this.$router.push('/home');\n        } else {\n          await this.$router.push('/admin/dashboard');\n        }\n      } catch (err) {\n        console.error('Login error:', err);\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.auth-page {\n  min-height: 100vh;\n  background: linear-gradient(135deg, #f5f7fa 0%, #e4e8f0 100%);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 2rem;\n  font-family: \"Poppins\", sans-serif;\n}\n\n.auth-container {\n  width: 100%;\n  max-width: 440px;\n}\n\n.auth-card {\n  background: white;\n  border-radius: 20px;\n  padding: 2.5rem;\n  box-shadow: 0 10px 25px -5px rgba(0, 0, 0, 0.1);\n  position: relative;\n}\n\n.auth-card::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 4px;\n  background: linear-gradient(90deg, #4f46e5, #7c3aed);\n  border-radius: 20px 20px 0 0;\n}\n\n.brand-section {\n  text-align: center;\n  margin-bottom: 2rem;\n}\n\n.brand {\n  font-size: 2rem;\n  font-weight: 700;\n  background: linear-gradient(135deg, #4f46e5 0%, #7c3aed 100%);\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n  margin-bottom: 0.5rem;\n}\n\n.tagline {\n  color: #64748b;\n  font-size: 0.95rem;\n}\n\n.auth-form {\n  display: flex;\n  flex-direction: column;\n  gap: 1.5rem;\n}\n\n.form-group {\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n}\n\n.form-group label {\n  font-weight: 600;\n  color: #1a202c;\n  font-size: 0.95rem;\n}\n\n.input-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n}\n\n.input-icon {\n  position: absolute;\n  left: 1rem;\n  font-size: 1.2rem;\n}\n\ninput {\n  width: 100%;\n  padding: 0.75rem 1rem 0.75rem 3rem;\n  border: 2px solid #e2e8f0;\n  border-radius: 12px;\n  font-size: 1rem;\n  color: #1a202c;\n  transition: all 0.3s ease;\n  background: #f8fafc;\n}\n\ninput:focus {\n  border-color: #4f46e5;\n  box-shadow: 0 0 0 3px rgba(79, 70, 229, 0.1);\n  outline: none;\n}\n\n.auth-button {\n  background: linear-gradient(135deg, #4f46e5 0%, #7c3aed 100%);\n  color: white;\n  padding: 1rem;\n  border: none;\n  border-radius: 12px;\n  font-weight: 600;\n  font-size: 1.1rem;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  margin-top: 1rem;\n}\n\n.auth-button:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 4px 6px -1px rgba(79, 70, 229, 0.2);\n}\n\n.error-message {\n  color: red;\n  font-size: 0.9rem;\n  text-align: center;\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAgB;;EACpBA,KAAK,EAAC;AAAW;;EAQbA,KAAK,EAAC;AAAY;;EAEhBA,KAAK,EAAC;AAAe;;EAavBA,KAAK,EAAC;AAAY;;EAEhBA,KAAK,EAAC;AAAe;;EA5BtCC,GAAA;EAyC4BD,KAAK,EAAC;;mBAzClC;;uBACEE,mBAAA,CAkDM,OAlDNC,UAkDM,GAjDJC,mBAAA,CAgDM,OAhDNC,UAgDM,GA/CJ