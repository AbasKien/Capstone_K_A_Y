{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: null,\n      isLoading: false\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n      this.isLoading = true;\n      this.error = null;\n      try {\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        localStorage.setItem('userEmail', response.data.email);\n        localStorage.setItem('userRole', response.data.role);\n        localStorage.setItem('token', response.data.token);\n        localStorage.setItem('isLoggedIn', 'true');\n        await this.$router.push('/home');\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","isLoading","methods","handleLogin","response","post","withCredentials","localStorage","setItem","role","token","$router","push","err","message"],"sources":["C:\\laragon\\www\\capstone\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\n  <div class=\"auth-page\">\n    <div class=\"auth-container\">\n      <div class=\"auth-card\">\n        <!-- Brand Section -->\n        <div class=\"brand-section\">\n          <h1 class=\"brand\">Domus Architecture</h1>\n          <p class=\"tagline\">Securely manage and track your documents</p>\n        </div>\n\n        <div class=\"form-container\">\n          <!-- Document Submission Form -->\n          <div class=\"document-form\">\n            <h2>Submit Document (For Guest)</h2>\n            <form>\n              <div class=\"form-group\">\n                <label>From:</label>\n                <input type=\"text\" placeholder=\"Full Name\" required />\n              </div>\n              <div class=\"form-group\">\n                <label>Office:</label>\n                <input type=\"text\" placeholder=\"Office\" required />\n              </div>\n              <div class=\"form-group\">\n                <label>Document Type:</label>\n                <select>\n                  <option value=\"\">--Select--</option>\n                  <option value=\"design\">Design Proposal</option>\n                  <option value=\"contract\">Contract</option>\n                  <option value=\"invoice\">Invoice</option>\n                </select>\n              </div>\n              <div class=\"form-group\">\n                <label>Details:</label>\n                <textarea placeholder=\"Description, Date, Destination\"></textarea>\n              </div>\n              <div class=\"form-group\">\n                <label>Purpose:</label>\n                <input type=\"text\" placeholder=\"Purpose or Action\" required />\n              </div>\n              <div class=\"form-group\">\n                <label>To:</label>\n                <select>\n                  <option value=\"receiving\">Receiving Office</option>\n                </select>\n              </div>\n              <button type=\"submit\" class=\"auth-button\">Submit</button>\n            </form>\n          </div>\n\n          <!-- Login Form -->\n          <div class=\"login-form\">\n            <h2>Log In (For Registered Users)</h2>\n            <form @submit.prevent=\"handleLogin\">\n              <div class=\"form-group\">\n                <label>Email</label>\n                <input type=\"email\" v-model=\"email\" required placeholder=\"Enter your email\" />\n              </div>\n              <div class=\"form-group\">\n                <label>Password</label>\n                <input type=\"password\" v-model=\"password\" required placeholder=\"Enter your password\" />\n              </div>\n              <div v-if=\"error\" class=\"error-message\">{{ error }}</div>\n              <button type=\"submit\" class=\"auth-button\" :disabled=\"isLoading\">{{ isLoading ? 'Logging in...' : 'Sign in' }}</button>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: null,\n      isLoading: false,\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n      this.isLoading = true;\n      this.error = null;\n      try {\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        }, { withCredentials: true });\n        localStorage.setItem('userEmail', response.data.email);\n        localStorage.setItem('userRole', response.data.role);\n        localStorage.setItem('token', response.data.token);\n        localStorage.setItem('isLoggedIn', 'true');\n        await this.$router.push('/home');\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.auth-page {\n  min-height: 100vh;\n  background: #0a192f;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 2rem;\n  font-family: \"Poppins\", sans-serif;\n  color: white;\n}\n\n.auth-container {\n  width: 100%;\n  max-width: 900px;\n}\n\n.auth-card {\n  background: #112240;\n  border-radius: 12px;\n  padding: 2rem;\n  box-shadow: 0 10px 25px -5px rgba(0, 0, 0, 0.3);\n}\n\n.brand {\n  font-size: 2rem;\n  font-weight: 700;\n  color: #64ffda;\n  text-align: center;\n}\n\n.tagline {\n  text-align: center;\n  color: #ccd6f6;\n  margin-bottom: 2rem;\n}\n\n.form-container {\n  display: flex;\n  gap: 2rem;\n}\n\n.document-form, .login-form {\n  flex: 1;\n  background: #233554;\n  padding: 1.5rem;\n  border-radius: 10px;\n}\n\nh2 {\n  font-size: 1.5rem;\n  color: #64ffda;\n  margin-bottom: 1rem;\n}\n\n.form-group {\n  margin-bottom: 1rem;\n}\n\nlabel {\n  font-weight: 600;\n  display: block;\n  margin-bottom: 0.5rem;\n}\n\ninput, select, textarea {\n  width: 100%;\n  padding: 0.75rem;\n  border: 1px solid #8892b0;\n  border-radius: 6px;\n  background: #112240;\n  color: white;\n}\n\nttextarea {\n  height: 80px;\n}\n\n.auth-button {\n  background: #64ffda;\n  color: #112240;\n  padding: 0.75rem;\n  border-radius: 6px;\n  font-weight: 600;\n  cursor: pointer;\n  transition: 0.3s;\n}\n\n.auth-button:hover {\n  opacity: 0.8;\n}\n\n.error-message {\n  color: #ff6363;\n  text-align: center;\n  margin-top: 0.5rem;\n}\n</style>"],"mappings":";AAyEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACL,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjC,IAAI,CAACC,KAAI,GAAI,2BAA2B;QACxC;MACF;MACA,IAAI,CAACC,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACD,KAAI,GAAI,IAAI;MACjB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,6BAA6B,EAAE;UAC/DP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,EAAE;UAAEO,eAAe,EAAE;QAAK,CAAC,CAAC;QAC7BC,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEJ,QAAQ,CAACP,IAAI,CAACC,KAAK,CAAC;QACtDS,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEJ,QAAQ,CAACP,IAAI,CAACY,IAAI,CAAC;QACpDF,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEJ,QAAQ,CAACP,IAAI,CAACa,KAAK,CAAC;QAClDH,YAAY,CAACC,OAAO,CAAC,YAAY,EAAE,MAAM,CAAC;QAC1C,MAAM,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;MAClC,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAACb,KAAI,GAAIa