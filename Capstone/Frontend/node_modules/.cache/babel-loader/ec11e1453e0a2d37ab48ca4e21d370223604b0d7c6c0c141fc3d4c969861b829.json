{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: null,\n      isLoading: false\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n      this.isLoading = true;\n      this.error = null;\n      try {\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        localStorage.setItem('userEmail', response.data.email);\n        localStorage.setItem('userRole', response.data.role);\n        localStorage.setItem('token', response.data.token);\n        localStorage.setItem('isLoggedIn', 'true');\n        const role = response.data.role;\n        const allowedRoles = ['admin', 'Lead architect', 'Secretary', 'Book keeper', 'Liaison', 'Apprentice'];\n        if (allowedRoles.includes(role)) {\n          await this.$router.push('/home');\n        } else {\n          await this.$router.push('/admin/dashboard');\n        }\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","isLoading","methods","handleLogin","response","post","withCredentials","localStorage","setItem","role","token","allowedRoles","includes","$router","push","err","message"],"sources":["C:\\laragon\\www\\capstone\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\n  <div class=\"auth-page\">\n    <div class=\"auth-container\">\n      <div class=\"auth-card\">\n        <div class=\"brand-section\">\n          <h1 class=\"brand\">Domus Architecture</h1>\n          <p class=\"tagline\">Welcome! Please sign in to continue.</p>\n        </div>\n\n        <form @submit.prevent=\"handleLogin\" class=\"auth-form\">\n          <div class=\"form-group\">\n            <label for=\"email\">Email Address</label>\n            <div class=\"input-wrapper\">\n              <span class=\"input-icon\">ðŸ“§</span>\n              <input\n                type=\"email\"\n                id=\"email\"\n                v-model=\"email\"\n                required\n                placeholder=\"Enter your email\"\n                autocomplete=\"email\"\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group\">\n            <label for=\"password\">Password</label>\n            <div class=\"input-wrapper\">\n              <span class=\"input-icon\">ðŸ”’</span>\n              <input\n                type=\"password\"\n                id=\"password\"\n                v-model=\"password\"\n                required\n                placeholder=\"Enter your password\"\n                autocomplete=\"current-password\"\n              />\n            </div>\n          </div>\n\n          <div v-if=\"error\" class=\"error-message\">\n            {{ error }}\n          </div>\n\n          <button type=\"submit\" class=\"auth-button\" :disabled=\"isLoading\">\n            {{ isLoading ? 'Logging in...' : 'Sign In' }}\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: null,\n      isLoading: false,\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n      \n      this.isLoading = true;\n      this.error = null;\n\n      try {\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n\n        localStorage.setItem('userEmail', response.data.email);\n        localStorage.setItem('userRole', response.data.role);\n        localStorage.setItem('token', response.data.token);\n        localStorage.setItem('isLoggedIn', 'true');\n\n        const role = response.data.role;\n        const allowedRoles = ['admin', 'Lead architect', 'Secretary', 'Book keeper', 'Liaison', 'Apprentice'];\n        if (allowedRoles.includes(role)) {\n          await this.$router.push('/home');\n        } else {\n          await this.$router.push('/admin/dashboard');\n        }\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.auth-page {\n  min-height: 100vh;\n  background: linear-gradient(135deg, #f3f4f6 0%, #e5e7eb 100%);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 2rem;\n  font-family: \"Poppins\", sans-serif;\n}\n\n.auth-container {\n  width: 100%;\n  max-width: 440px;\n}\n\n.auth-card {\n  background: white;\n  border-radius: 12px;\n  padding: 2rem;\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);\n  text-align: center;\n}\n\n.brand {\n  font-size: 1.8rem;\n  font-weight: bold;\n  color: #2c3e50;\n}\n\n.tagline {\n  color: #64748b;\n  font-size: 1rem;\n  margin-bottom: 1.5rem;\n}\n\n.form-group {\n  text-align: left;\n  margin-bottom: 1rem;\n}\n\nlabel {\n  font-weight: 600;\n  color: #374151;\n}\n\n.input-wrapper {\n  display: flex;\n  align-items: center;\n  background: #f9fafb;\n  border: 1px solid #d1d5db;\n  border-radius: 8px;\n  padding: 0.75rem;\n}\n\n.input-icon {\n  margin-right: 0.5rem;\n  color: #6b7280;\n}\n\ninput {\n  flex: 1;\n  border: none;\n  background: transparent;\n  font-size: 1rem;\n  outline: none;\n}\