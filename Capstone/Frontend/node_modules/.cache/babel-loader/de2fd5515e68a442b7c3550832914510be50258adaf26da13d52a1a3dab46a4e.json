{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: null,\n      isLoading: false\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n      this.isLoading = true;\n      this.error = null;\n      try {\n        const response = await axios.post(\"http://localhost:5555/login\", {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        if (response.data.needsVerification) {\n          this.error = response.data.message;\n          return;\n        }\n        localStorage.setItem(\"userEmail\", response.data.email);\n        localStorage.setItem(\"userRole\", response.data.role);\n        localStorage.setItem(\"token\", response.data.token);\n        localStorage.setItem(\"isLoggedIn\", \"true\");\n        const role = response.data.role;\n        const allowedRoles = [\"admin\", \"Lead architect\", \"Secretary\", \"Book keeper\", \"Liaison\", \"Apprentice\"];\n        if (allowedRoles.includes(role)) {\n          await this.$router.push(\"/home\");\n        } else {\n          await this.$router.push(\"/admin/dashboard\");\n        }\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","isLoading","methods","handleLogin","response","post","withCredentials","needsVerification","message","localStorage","setItem","role","token","allowedRoles","includes","$router","push","err"],"sources":["C:\\laragon\\www\\capstone\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\n  <div class=\"auth-page\">\n    <div class=\"auth-container\">\n      <div class=\"form-wrapper\">\n        <!-- Document Submission Form -->\n        <div class=\"document-form\">\n          <h2 class=\"form-title\">Submit Document (For Guest)</h2>\n          <form>\n            <div class=\"form-group\">\n              <label>From:</label>\n              <input type=\"text\" placeholder=\"Full Name\" required />\n            </div>\n            <div class=\"form-group\">\n              <label>Office:</label>\n              <input type=\"text\" placeholder=\"Office\" required />\n            </div>\n            <div class=\"form-group\">\n              <label>Document Type:</label>\n              <select required>\n                <option value=\"\">--Select--</option>\n                <option value=\"Report\">Report</option>\n                <option value=\"Blueprint\">Blueprint</option>\n              </select>\n            </div>\n            <div class=\"form-group\">\n              <label>Details:</label>\n              <textarea placeholder=\"Description, Date, Destination\" required></textarea>\n            </div>\n            <div class=\"form-group\">\n              <label>Purpose:</label>\n              <input type=\"text\" placeholder=\"Purposes or Actions to be taken...\" required />\n            </div>\n            <div class=\"form-group\">\n              <label>To:</label>\n              <input type=\"text\" placeholder=\"Receiving Office\" required />\n            </div>\n            <div class=\"form-buttons\">\n              <button type=\"reset\" class=\"btn btn-cancel\">Cancel</button>\n              <button type=\"submit\" class=\"btn btn-submit\">Submit</button>\n            </div>\n          </form>\n        </div>\n\n        <!-- Login Form -->\n        <div class=\"auth-card\">\n          <h2 class=\"form-title\">Log In (For Registered Users)</h2>\n          <form @submit.prevent=\"handleLogin\" class=\"auth-form\">\n            <div class=\"form-group\">\n              <label for=\"email\">Email</label>\n              <input\n                type=\"email\"\n                id=\"email\"\n                v-model=\"email\"\n                required\n                placeholder=\"Enter your email\"\n              />\n            </div>\n            <div class=\"form-group\">\n              <label for=\"password\">Password</label>\n              <input\n                type=\"password\"\n                id=\"password\"\n                v-model=\"password\"\n                required\n                placeholder=\"Enter your password\"\n              />\n            </div>\n            <div v-if=\"error\" class=\"error-message\">{{ error }}</div>\n            <div class=\"form-buttons\">\n              <button type=\"submit\" class=\"btn btn-login\" :disabled=\"isLoading\">\n                {{ isLoading ? 'Logging in...' : 'Sign in' }}\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: null,\n      isLoading: false,\n    };\n  },\n  methods: {\n    async handleLogin() {\n      if (!this.email || !this.password) {\n        this.error = \"Please fill in all fields\";\n        return;\n      }\n\n      this.isLoading = true;\n      this.error = null;\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:5555/login\",\n          {\n            email: this.email,\n            password: this.password,\n          },\n          { withCredentials: true }\n        );\n\n        if (response.data.needsVerification) {\n          this.error = response.data.message;\n          return;\n        }\n\n        localStorage.setItem(\"userEmail\", response.data.email);\n        localStorage.setItem(\"userRole\", response.data.role);\n        localStorage.setItem(\"token\", response.data.token);\n        localStorage.setItem(\"isLoggedIn\", \"true\");\n\n        const role = response.data.role;\n        const allowedRoles = [\n          \"admin\",\n          \"Lead architect\",\n          \"Secretary\",\n          \"Book keeper\",\n          \"Liaison\",\n          \"Apprentice\",\n        ];\n        if (allowedRoles.includes(role)) {\n          await this.$router.push(\"/home\");\n        } else {\n          await this.$router.push(\"/admin/dashboard\");\n        }\n      } catch (err) {\n        this.error =\n          err.response?.data?.message || \"Login failed. Please try again.\";\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Page Layout */\n.auth-page {\n  min-height: 100vh;\n  background: #0a192f;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 2rem;\n  font-family: \"Poppins\", sans-serif;\n}\n\n.auth-container {\n  width: 90%;\n  max-width: 1100px;\n}\n\n/* Forms Wrapper */\n.form-wrapper {\n  display: flex;\n  gap: 2rem;\n  justify-content: space-between;\n}\n\n/* Forms */\n.document-form,\n.auth-card {\n  background: #112240;\n  padding: 2rem;\n  border-radius: 12px;\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);\n  width: 48%;\n  color: white;\n}\n\n.form-title {\n  font-size: 1.5rem;\n  font-weight: 600;\n  margin-bottom: 1rem;\n  color: #64ffda;\n}\n\n/* Form Fields */\n.form-group {\n  margin-bottom: 1rem;\n}\n\nlabel {\n  font-weight: 500;\n  display: block;\n  margin-bottom: 0.5rem;\n}\n\ninput,\ntextarea,\nselect {\n  width: 100%;\n  padding: 0.75rem;\n  border: 1px solid #64ffda;\n  border-radius: 8px;\n  background: #0a192f;\n  color: white;\n  font-size: 1rem;\n}\n\ntextarea {\n  height: 80px;\n}\n\n/* Buttons */\n.form-buttons {\n  display: flex;\n  justify-content: space-between;\n}\n\n.btn {\n  padding: 0.75rem 1.5rem;\n  border: none;\n  border-radius: 8px;\n  font-size: 1rem;\n  cursor: pointer;\n  font-weight: 600;\n  transition: all 0.3s ease;\n}\n\n.btn-cancel {\n  background: #ff6b6b;\n  color: white;\n}\n\n.btn-submit {\n  background: #64ffda;\n  color: #0a192f;\n}\n\n.btn-login {\n  background: #64ffda;\n  color: #0a192f;\n  width: 100%;\n}\n\n\n\n/* Error Message */\n.error-message {\n  color: red;\n  font-size: 0.9rem;\n  text-align: center;\n  margin-top: 0.5rem;\n}\n\n/* Responsive Design */\n@media (max-width: 768px) {\n  .form-wrapper {\n    flex-direction: column;\n  }\n  .document-form,\n  .auth-card {\n    width: 100%;\n  }\n}\n</style>\n"],"mappings":";AAiFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACL,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjC,IAAI,CAACC,KAAI,GAAI,2BAA2B;QACxC;MACF;MAEA,IAAI,CAACC,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACD,KAAI,GAAI,IAAI;MAEjB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAC/B,6BAA6B,EAC7B;UACEP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,EACD;UAAEO,eAAe,EAAE;QAAK,CAC1B,CAAC;QAED,IAAIF,QAAQ,CAACP,IAAI,CAACU,iBAAiB,EAAE;UACnC,IAAI,CAACP,KAAI,GAAII,QAAQ,CAACP,IAAI,CAACW,OAAO;UAClC;QACF;QAEAC,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEN,QAAQ,CAACP,IAAI,CAACC,KAAK,CAAC;QACtDW,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEN,QAAQ,CAACP,IAAI,CAACc,IAAI,CAAC;QACpDF,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEN,QAAQ,CAACP,IAAI,CAACe,KAAK,CAAC;QAClDH,YAAY,CAACC,OAAO,CAAC,YAAY,EAAE,MAAM,CAAC;QAE1C,MAAMC,IAAG,GAAIP,QAAQ,CAACP,IAAI,CAACc,IAAI;QAC/B,MAAME,YAAW,GAAI,CACnB,OAAO,EACP,gBAAgB,EAChB,WAAW,EACX,aAAa,EACb,SAAS,EACT,YAAY,CACb;QACD,IAAIA,YAAY,CAACC,QAAQ,CAACH,IAAI,CAAC,EAAE;UAC/B,MAAM,IAAI,CAACI,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;QAClC,OAAO;UACL,MAAM,IAAI,CAACD,OAAO,CAACC,IAAI,CAAC,kBAAkB,CAAC;QAC7C;MACF,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAACjB,KAAI,GACPiB,GAAG,CAACb,QAAQ,EAAEP,IAAI,EAAEW,OAAM,IAAK,iCAAiC;MACpE,UAAU;QACR,IAAI,CA